
<h1 class="app-main-title">Hubleto Cloud > Usage log</h1>

<table class="table-default dense">
  <thead>
    <tr>
      <th>Datetime</th>
      <th>Active users</th>
      <th>Paid apps</th>
    </tr>
  </thead>
  <tbody>
    {% for log in viewParams.log %}
      <tr>
        <td>{{ log.log_datetime }}</td>
        <td>{{ log.active_users }}</td>
        <td>{{ log.paid_apps }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
