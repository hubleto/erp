{
    "name": "hubleto/erp",
    "license": "GPL-3.0-only",
    "description": "Business Application Hub. Opensource ERP and CRM.",
    "homepage": "https://www.hubleto.com",
    "support": {
        "issues": "https://github.com/hubleto/erp/issues",
        "source": "https://github.com/hubleto/erp"
    },
    "require": {
        "hubleto/framework": "0.27",
        "hubleto/pay-by-square-encoder": "0.27",
        "defuse/php-encryption": "^2",
        "ddeboer/imap": "^1",
        "endroid/qr-code": "^6",
        "dompdf/dompdf": "^3",
        "phpmailer/phpmailer": "^6"
    },
    "keywords": ["hubleto", "crm", "erp", "application", "hub"],
    "autoload": {
        "psr-4": {
            "Hubleto\\Erp\\": "src",
            "Hubleto\\Erp\\Cli\\": "cli",
            "Hubleto\\Erp\\EventListener\\": "event-listeners",
            "Hubleto\\Erp\\Cron\\": "crons",
            "Hubleto\\Erp\\Report\\": "reports",
            "Hubleto\\Erp\\Installer\\": "installer",
            "Hubleto\\App\\Community\\": "apps"
        }
    },
    "minimum-stability": "dev",
    "bin": [
        "hubleto"
    ]
}
