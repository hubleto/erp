<div class="grid grid-cols-2 gap-2">
  <div class="card">
    <div class="card-header">
      Current time
    </div>
    <div class="card-body">
      {{ '{{ ' }} viewParams.now {{ '}} ' }}
    </div>
  </div>
  <div class="card">
    <div class="card-header">
      Random number
    </div>
    <div class="card-body">
      {{ '{{ ' }} viewParams.randomNumber {{ '}} ' }}
    </div>
  </div>
</div>