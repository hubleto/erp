{
    "name": "hubleto/main",
    "license": "GPL-3.0-only",
    "description": "Business Application Hub. Opensource ERP and CRM.",
    "homepage": "https://www.hubleto.com",
    "support": {
        "issues": "https://github.com/hubleto/main/issues",
        "source": "https://github.com/hubleto/main"
    },
    "require": {
        "hubleto/terminal": "@dev",
        "hubleto/framework": "@dev",
        "hubleto/apps": "@dev"
    },
    "keywords": ["hubleto", "crm", "erp", "application", "hub"],
    "autoload": {
        "psr-4": {
            "HubletoMain\\": "src",
            "HubletoMain\\Cli\\": "cli",
            "HubletoMain\\Hook\\": "hooks",
            "HubletoMain\\Cron\\": "crons",
            "HubletoMain\\Report\\": "reports",
            "HubletoMain\\Installer\\": "installer"
        }
    },
    "minimum-stability": "dev",
    "bin": [
        "hubleto",
        "hubleto-make-production-release"
    ]
}
